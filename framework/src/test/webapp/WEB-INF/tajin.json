{
    "debug": true,
    "client": {
        "modules": {
            "log": {
                "level": "all"
            },
            "app": {
                "ws": {
                    "api": "http://${domain}/api",
                    "root": "http://${domain}"
                },
                "mocked": $mocked
            },

            "i18n": {
                "debug": true,
                "attributes": ["href", "src"],
                "bundles": {
                    "mybnd": {
                        "location": "js/i18n",
                        "preload": ["en"]
                    }
                }
            }
        }
    }
}
