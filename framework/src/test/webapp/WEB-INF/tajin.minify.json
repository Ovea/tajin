{
    "debug": true,

    "client": {
        "modules": {
            "log": {
                "level": "all"
            },
            "app": {
                "ws": {
                    "api": "http://${domain}/api",
                    "root": "http://${domain}"
                },
                "mocked": $mocked
            },

            "i18n": {
                "debug": true,
                "attributes": ["href", "src"],
                "bundles": {
                    "mybnd": {
                        "location": "js/i18n",
                        "preload": ["en"]
                    },
                    "mybnd2": {
                        "location": "http://remote",
                        "preload": ["en"]
                    }
                }
            }
        }
    },

    "minify": {
        "includes": [
            "js/**/*.js",
            "css/**/*.css",
            "css/inexisting.css"
        ],
        "excludes": []
    }

}
